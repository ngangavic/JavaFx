   <children>
      <Label layoutX="64.0" layoutY="14.0" text="Add Class">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <TextField fx:id="tfClassName" layoutX="28.0" layoutY="57.0" prefHeight="25.0" prefWidth="154.0" promptText="Enter class name" />
      <Button layoutX="45.0" layoutY="107.0" mnemonicParsing="false" onAction="#insertClass" text="Add" />
      <Button layoutX="110.0" layoutY="107.0" mnemonicParsing="false" text="View" />
   </children>
</AnchorPane>
