<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="342.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Users.Login">
   <children>
      <Label layoutX="119.0" layoutY="14.0" text="SYSTEM LOGIN">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Label layoutX="23.0" layoutY="58.0" text="Username" />
      <TextField fx:id="username" layoutX="85.0" layoutY="54.0" prefHeight="25.0" prefWidth="250.0" promptText="enter your username" />
      <PasswordField fx:id="password" layoutX="85.0" layoutY="107.0" prefHeight="25.0" prefWidth="250.0" promptText="enter your password" />
      <Label layoutX="23.0" layoutY="111.0" text="Password" />
      <Button layoutX="85.0" layoutY="163.0" mnemonicParsing="false" onAction="#loginClick" prefHeight="25.0" prefWidth="250.0" text="Login" />
      <Button layoutX="85.0" layoutY="212.0" mnemonicParsing="false" onAction="#clickRegister" prefHeight="25.0" prefWidth="250.0" text="Register" />
      <Button layoutX="85.0" layoutY="263.0" mnemonicParsing="false" onAction="#clickResetPassword" prefHeight="25.0" prefWidth="250.0" text="Reset Password" />
   </children>
</AnchorPane>
